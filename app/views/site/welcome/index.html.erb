<h1>Site::Welcome#index</h1>
<p>Find me in app/views/site/welcome/index.html.erb</p>

<% if user_signed_in? %>
  <p>Usuário está logado como: <%= current_user.email  %></p>
  
<% else %>
  <p>Usuário não está logado</p>
<% end %>

<% if admin_signed_in? %>
  <p>Administrador está logado como: <%= current_admin.email  %></p>
<% else %>
  <p>Administrador não está logado</p>
<% end %>

<p><%= link_to 'Área do usuário', users_backoffice_welcome_index_path %></p>

<p><%= link_to 'Área do admin', admins_backoffice_welcome_index_path %></p>